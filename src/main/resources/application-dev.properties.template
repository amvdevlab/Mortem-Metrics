# PostgreSQL Database Configuration
# INSTRUCTIONS: Copy this file to application-dev.properties and fill in your credentials
spring.datasource.url=jdbc:postgresql://YOUR_SUPABASE_HOST:5432/postgres?sslmode=require
spring.datasource.username=postgres
spring.datasource.password=YOUR_DATABASE_PASSWORD

# Supabase Object Storage Connection
supabase.url=https://YOUR_PROJECT_ID.supabase.co
supabase.key=YOUR_SERVICE_ROLE_KEY
supabase.storage.bucket=incident-attachments